<%= render partial: "shared/user", locals: { user: @user } %>
<% @posts.each do |post| %>
  <%= render partial: "shared/post", locals: { post: post } %>
  <%= render partial: "shared/comments", locals: { post: post } %>
  <%= link_to 'New Comment', new_comment_path(post.id) %>
  <%= link_to 'Add Like', create_like_path(post.id) %>
<% end %>
<br>
<%= link_to 'New Post', new_post_path %>